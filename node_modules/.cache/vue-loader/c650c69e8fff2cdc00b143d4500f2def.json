sourceRoot":"","sourcesContent":["<template>\r\n  <div\r\n    class=\"divider\"\r\n    :style=\"{ left: `${dividerPosition}%` }\"\r\n    @mousedown=\"startDrag\"\r\n    @touchstart=\"startDrag\"\r\n  ></div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      dragging: false,\r\n      initialX: 0,\r\n      initialPosition: 50 // Initial position of the divider (in percentage)\r\n    };\r\n  },\r\n  computed: {\r\n    dividerPosition() {\r\n      return this.dragging ? this.calculatePosition() : this.initialPosition;\r\n    }\r\n  },\r\n  methods: {\r\n    startDrag(event) {\r\n      event.preventDefault();\r\n      this.dragging = true;\r\n      this.initialX = event.clientX || event.touches[0].clientX;\r\n      window.addEventListener('mousemove', this.drag);\r\n      window.addEventListener('touchmove', this.drag);\r\n      window.addEventListener('mouseup', this.stopDrag);\r\n      window.addEventListener('touchend', this.stopDrag);\r\n    },\r\n    stopDrag() {\r\n      this.dragging = false;\r\n      window.removeEventListener('mousemove', this.drag);\r\n      window.removeEventListener('touchmove', this.drag);\r\n      window.removeEventListener('mouseup', this.stopDrag);\r\n      window.removeEventListener('touchend', this.stopDrag);\r\n    },\r\n    drag(event) {\r\n      event.preventDefault();\r\n      const clientX = event.clientX || event.touches[0].clientX;\r\n      const dx = clientX - this.initialX;\r\n      const newPosition = this.initialPosition + (dx / window.innerWidth) * 100;\r\n      this.initialX = clientX;\r\n      this.initialPosition = Math.max(0, Math.min(100, newPosition));\r\n    },\r\n    calculatePosition() {\r\n      return Math.max(0, Math.min(100, this.initialPosition));\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.divider {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  width: 5px; /* Adjust the width of the divider */\r\n  background-color: #ccc; /* Adjust the color of the divider */\r\n  cursor: ew-resize; /* Set the cursor type */\r\n}\r\n</style>"]}]}